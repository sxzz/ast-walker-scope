{
  "name": "ast-walker-scope",
  "version": "0.5.0",
  "packageManager": "pnpm@8.8.0",
  "description": "Traverse Babel AST with scope information.",
  "license": "MIT",
  "homepage": "https://github.com/sxzz/ast-walker-scope#readme",
  "bugs": {
    "url": "https://github.com/sxzz/ast-walker-scope/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sxzz/ast-walker-scope.git"
  },
  "author": "三咲智子 <sxzz@sxzz.moe>",
  "files": [
    "dist"
  ],
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": {
        "require": "./dist/index.d.ts",
        "import": "./dist/index.d.mts"
      },
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    },
    "./*": "./*"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "lint": "eslint .",
    "lint:fix": "pnpm run lint --fix",
    "build": "tsup",
    "test": "vitest",
    "release": "bumpp && pnpm publish",
    "prepublishOnly": "pnpm run build"
  },
  "dependencies": {
    "@babel/parser": "^7.23.0",
    "ast-kit": "^0.11.2"
  },
  "devDependencies": {
    "@babel/types": "^7.23.0",
    "@sxzz/eslint-config": "^3.6.1",
    "@sxzz/prettier-config": "^1.0.4",
    "@types/node": "^20.8.0",
    "bumpp": "^9.2.0",
    "eslint": "^8.50.0",
    "eslint-define-config": "^1.23.0",
    "fast-glob": "^3.3.1",
    "magic-string": "^0.30.4",
    "prettier": "^3.0.3",
    "tsup": "^7.2.0",
    "tsx": "^4.1.1",
    "typescript": "^5.2.2",
    "vite": "^4.4.9",
    "vitest": "^0.34.6"
  },
  "engines": {
    "node": ">=16.14.0"
  },
  "prettier": "@sxzz/prettier-config"
}
