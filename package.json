{
  "name": "ast-walker-scope",
  "version": "0.0.0",
  "packageManager": "pnpm@7.6.0",
  "description": "Traverse Babel AST with scope information.",
  "license": "MIT",
  "files": [
    "dist"
  ],
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    "require": "./dist/index.js",
    "import": "./dist/index.mjs"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "lint": "eslint . --ext .vue,.js,.ts,.jsx,.tsx,.json,.md",
    "lint:fix": "pnpm run lint -- --fix",
    "build": "tsup && tsx scripts/postbuild.mts",
    "test": "vitest",
    "release": "bumpp --commit --push --tag && pnpm publish",
    "prepublishOnly": "pnpm run build"
  },
  "dependencies": {
    "@babel/parser": "^7.18.9",
    "@babel/types": "^7.18.9",
    "estree-walker": "^3.0.1"
  },
  "devDependencies": {
    "@sxzz/eslint-config-prettier": "^2.3.4",
    "@sxzz/eslint-config-ts": "^2.3.4",
    "@types/node": "*",
    "bumpp": "^8.2.1",
    "eslint": "^8.20.0",
    "eslint-define-config": "^1.5.1",
    "fast-glob": "^3.2.11",
    "magic-string": "^0.26.2",
    "prettier": "^2.7.1",
    "tsup": "^6.1.3",
    "tsx": "^3.8.0",
    "typescript": "^4.7.4",
    "vite": "^3.0.3",
    "vitest": "^0.19.1"
  },
  "engines": {
    "node": ">=14.19.0"
  }
}
