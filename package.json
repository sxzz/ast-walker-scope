{
  "name": "ast-walker-scope",
  "version": "0.4.1",
  "packageManager": "pnpm@8.6.0",
  "description": "Traverse Babel AST with scope information.",
  "repository": "sxzz/ast-walker-scope",
  "license": "MIT",
  "files": [
    "dist"
  ],
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    },
    "./*": "./*"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "lint": "eslint . --ext .vue,.js,.ts,.jsx,.tsx,.json,.md",
    "lint:fix": "pnpm run lint --fix",
    "build": "tsup",
    "test": "vitest",
    "release": "bumpp && pnpm publish",
    "prepublishOnly": "pnpm run build"
  },
  "dependencies": {
    "@babel/parser": "^7.22.4",
    "@babel/types": "^7.22.4"
  },
  "devDependencies": {
    "@sxzz/eslint-config-prettier": "^2.4.6",
    "@sxzz/eslint-config-ts": "^2.4.6",
    "@types/node": "^18.16.16",
    "bumpp": "^9.1.0",
    "eslint": "^8.42.0",
    "eslint-define-config": "^1.20.0",
    "estree-walker": "^3.0.3",
    "fast-glob": "^3.2.12",
    "magic-string": "^0.30.0",
    "prettier": "^2.8.8",
    "tsup": "^6.7.0",
    "tsx": "^3.12.7",
    "typescript": "^5.1.3",
    "vite": "^4.3.9",
    "vitest": "^0.31.4"
  },
  "engines": {
    "node": ">=16.14.0"
  }
}
